
pragma solidity >=0.5.0 <0.9.0;

// Creating a Smart Contract
contract Structreq{

// Structure of employee
struct allocreq{
	// State variables
    uint bid;
	string fid;
	string  tid;
	string  mid;
	string qty;
	string date;
	string status;
}
allocreq []emps;
// Function to add
// addrecords details
function addreq(uint bid, string memory fid,string memory tid,string memory mid,string memory qty,string memory date,string memory status) public{
	allocreq memory e =allocreq(bid,fid,tid,mid,qty,date,status);
	emps.push(e);
}

}
